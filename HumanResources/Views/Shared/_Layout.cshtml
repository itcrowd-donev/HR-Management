<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="~/Content/image/ico" sizes="16x16" href="~/Content/images/favicon.ico">
    <title>Human Resources Manager</title>
    <!-- Bootstrap Core CSS -->
    <link href="~/Content/css/lib/bootstrap/bootstrap.min.css" rel="stylesheet">
    <!-- Custom CSS -->
    <link href="~/Content/css/lib/calendar2/semantic.ui.min.css" rel="stylesheet">
    <link href="~/Content/css/lib/calendar2/pignose.calendar.min.css" rel="stylesheet">
    <link href="~/Content/css/lib/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/Content/css/lib/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/Content/css/helper.css" rel="stylesheet">
    <link href="~/Content/css/style.css" rel="stylesheet">
    <link href="~/Content/css/lib/datepicker/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:** -->
    <!--[if lt IE 9]>
        <script src="https:**oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https:**oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>



<body class="fix-header fix-sidebar">

    <!-- Preloader - style you can find in spinners.css -->
    <div class="preloader">
        <svg class="circular" viewBox="25 25 50 50">
            <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="2" stroke-miterlimit="10" />
        </svg>
    </div>
    <!-- Main wrapper  -->
    <div id="main-wrapper">
        <!-- header header  -->
        <div class="header">
            <nav class="navbar top-navbar navbar-expand-md navbar-light">
                <!-- Logo -->
                <div class="navbar-header">
                    <a class="navbar-brand">
                        <!-- Logo icon -->
                        <b><img src="~/Content/images/HR.jpg" alt="homepage" class="dark-logo" width="200" height="50" /></b>
                        <!--End Logo icon -->
                        <!-- Logo text -->
                    </a>
                </div>
                <!-- End Logo -->
                @{Html.RenderPartial("_NavbarTop");}
            </nav>
        </div>
        <!-- End header header -->
        <!-- Left Sidebar  -->
        @{Html.RenderPartial("_MenuItems");}
        <!-- End Left Sidebar  -->
        <!-- Page wrapper  -->
        <div class="page-wrapper">
            <!-- Bread crumb -->
            <!-- End Bread crumb -->
            <!-- Container fluid  -->
            <div class="container-fluid">
                <!-- Start Page Content -->
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body"> @RenderBody()</div>
                        </div>
                    </div>
                </div>
                <!-- End PAge Content -->
            </div>
            <!-- End Container fluid  -->
            <!-- footer -->
            <footer class="footer"> © 2018 All rights reserved. Template implemented by <a href="http://itcrowd.me/">ItCrowd</a></footer>
            <!-- End footer -->
        </div>
        <!-- End Page wrapper  -->
    </div>
    <!-- End Wrapper -->
    <!-- All Jquery -->
    <script src="~/Scripts/js/lib/jquery/jquery.min.js"></script>
    <!-- Bootstrap tether Core JavaScript -->
    <script src="~/Scripts/js/lib/bootstrap/js/popper.min.js"></script>

    <script src="~/Scripts/bootstrap.js"></script>
    <!-- slimscrollbar scrollbar JavaScript -->
    <script src="~/Scripts/js/jquery.slimscroll.js"></script>
    <!--Menu sidebar -->
    <script src="~/Scripts/js/sidebarmenu.js"></script>
    <!--stickey kit -->
    <script src="~/Scripts/js/lib/sticky-kit-master/dist/sticky-kit.min.js"></script>
    <script src="~/Scripts/js/lib/calendar-2/moment.latest.min.js"></script>

    <!-- scripit init-->

    @*<script src="~/Scripts/js/lib/calendar-2/semantic.ui.min.js"></script>*@
    <script src="~/Scripts/js/lib/calendar-2/prism.min.js"></script>
    <script src="~/Scripts/js/lib/calendar-2/pignose.calendar.min.js"></script>
    <script src="~/Scripts/js/lib/calendar-2/pignose.init.js"></script>

    <script src="~/Scripts/js/lib/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/Scripts/js/lib/owl-carousel/owl.carousel-init.js"></script>

    <script src="~/Scripts/js/scripts.js"></script>
    <!-- scripit init-->

    <!-- data tables-->
    <script src="~/Scripts/js/lib/datatables/datatables.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdn.datatables.net/buttons/1.2.2/js/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdn.datatables.net/buttons/1.2.2/js/buttons.flash.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdnjs.cloudflare.com/ajax/libs/jszip/2.5.0/jszip.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/pdfmake.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/vfs_fonts.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdn.datatables.net/buttons/1.2.2/js/buttons.html5.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/cdn.datatables.net/buttons/1.2.2/js/buttons.print.min.js"></script>
    <script src="~/Scripts/js/lib/datatables/datatables-init.js"></script>
    <!-- end data tables-->

    <!-- Calendars-->
    <script src="~/Scripts/js/lib/datepicker/bootstrap-datepicker.min.js"></script>
    <script src="~/Scripts/js/bootstrap-year-calendar.min.js"></script>
    <script src="~/Scripts/js/bootstrap-year-calendar-config.js"></script>
    <!-- end Calendars-->
    @RenderSection("scripts", required: false)
</body>
</html>


